    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "autoprefixer": "^7.2.5",
    "bluebird": "^3.3.4",
    "classnames": "^2.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-dock": "^0.2.4",
    "react-dom": "^16.4.0",
    "react-intl": "^2.4.0",
    "react-redux": "^4.4.9",
